{
  "name": "sample-website",
  "description": "SAMPLE Website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve:local": "vue-cli-service serve --mode local",
    "serve:host-dev": "npm run build:dev && cd host && npm run dev",
    "build": "vue-cli-service build --modern --mode production --verbose",
    "test:ui-local": "jest --logHeapUsage",
    "test:host": "cd host && npm run test",
    "test:ui-update": "npm run test:ui-local -- -update",
    "lint": "vue-cli-service lint",
    "lint:ui": "vue-cli-service lint",
    "lint:host": "cd host && npm run lint",
    "lint:js-fix": "vue-cli-service lint --fix",
    "lint:css-fix": "stylelint --fix ./*/.{vue,scss,css}",
    "lint:ui-fix": "npm run lint:js-fix && npm run lint:css-fix",
    "lint:all": "concurrently \"npm run lint:ui\" \"npm run lint:host\""
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.16.8",
    "awesome-json2json": "^0.6.0",
    "axios": "^0.21.4",
    "axios-helpers": "^1.3.0",
    "concurrently": "^7.0.0",
    "core-js": "^3.20.3",
    "dayjs": "^1.10.6",
    "object-dig": "^0.1.3",
    "rambda": "^7.0.1",
    "rbc-wm-framework-vuejs": "^1.6.20",
    "string-format": "^2.0.0",
    "vue": "^2.6.14",
    "vue-fragment": "1.5.1",
    "vue-json-pretty": "^1.8.2",
    "vue-router": "^3.5.3",
    "vuelidate": "^0.7.6",
    "vuelidate-error-extractor": "^2.4.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-vuex": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.6",
    "babel-plugin-transform-require-context": "^0.1.1",
    "flush-promises": "^1.0.2",
    "jest": "^27.4.7",
    "jest-serializer-vue-tjw": "^3.19.0",
    "jest-transform-stub": "^2.0.0",
    "jest-when": "^3.5.0",
    "node-sass": "^5.0.0",
    "opener": "^1.5.2",
    "pre-commit": "^1.2.2",
    "sass-loader": "^10.2.0",
    "sinon": "^12.0.1",
    "style-loader": "^1.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "vue-cli-plugin-compression": "^1.1.5",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.14"
  }
}
