{
  "extends": "airbnb",
  "env": {
  "node": true
},
  "rules": {
  "max-len": ["error", {
    "code": 120,
    "tabWidth": 2,
    "ignoreUrls": true,
    "ignoreComments": true,
    "ignoreTrailingComments": true
  }],
    "no-shadow": 0,
    "no-unused-expressions": 0,
    "no-unused-vars": ["error", {"args": "all", "argsIgnorePattern": "^_"}]
}
}
